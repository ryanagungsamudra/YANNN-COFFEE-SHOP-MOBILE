{
  "expo": {
    "name": "yannn-coffee-native",
    "slug": "yannn-coffee-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Kita Butuh Akses Galeri/kamera kamu buat pilih gambar.",
          "cameraPermission": "Kita Butuh Akses Galeri/kamera kamu buat pilih gambar."
        }
      ]
    ],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.yannn.coffeeshop"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "98b3658b-d700-4a0c-8f0a-b8937bee75e8"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/98b3658b-d700-4a0c-8f0a-b8937bee75e8"
    }
  }
}
